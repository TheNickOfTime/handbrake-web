{
  "name": "@handbrake-web/shared",
  "description": "HandBrake Web Shared",
  "scripts": {
    "build": "tsc -b",
    "postinstall": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "https://git.goodforyou.games/ncunningham/handbrake-web.git"
  },
  "exports": {
    "./dict/*": {
      "default": "./build/dict/*.js",
      "types": "./build/dict/*.d.ts"
    },
    "./funcs/*": {
      "default": "./build/funcs/*.js",
      "types": "./build/funcs/*.d.ts"
    },
    "./types/*": {
      "default": "./build/types/*.js",
      "types": "./build/types/*.d.ts"
    },
    "./logger": {
      "types": "./build/logger/index.d.ts",
      "default": "./build/logger/index.js"
    }
  },
  "dependencies": {
    "kysely": "^0.28.7",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "typescript": "^5.9.3"
  }
}
